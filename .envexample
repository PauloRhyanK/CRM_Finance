# Configurações do Flask
FLASK_APP=src/app.py
FLASK_DEBUG=1
FLASK_ENV=development

# Chave secreta (para sessions/JWT)
SECRET_KEY=sua-chave-super-secreta-aqui

# Configurações específicas do CRM
CRM_NAME=CRM Finance
CRM_VERSION=1.0.0

# Variáveis para o PostgreSQL
POSTGRES_DB=crm_db
POSTGRES_USER=postgres
POSTGRES_PASSWORD=1234

# Variável para a nossa aplicação Flask (ela usa as de cima para se conectar)
DATABASE_URL="postgresql://postgres:1234@db:5432/crm_db"